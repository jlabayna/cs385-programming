// Note: The program itself is an argument as a character array!
// Each string in argv ends in \0

// Note: werror turns warnings into errors

#include <iostream>
#include <math.h>
#include <bitset>
#include <string>
#include <regex>

using namespace std;

struct State {
	int a, b, c;
	string directions;
	State *parent;

	State(int _a, int _b, int _c, string _directions) :
			a { _a }, b { _b }, c { _c }, directions(_directions), parent {
					nullptr } {
	}

	// String representation of state in tuple form.
	string to_string() {
		ostringstream oss;
		oss << "(" << a << ", " << b << ", " << c << ")";
		return oss.str();
	}

	bool equals(State *s) {
		return a == s->a && b == s->b && c == s->c;
	}
};

/**
 * Outputs a range of integers for n which satisfies 4n^3 > 64nlg(n)
 */
int main(int argc, char *argv[]) {
	State *test = new State(1,1,1,"");
	int a = test->b;
	int b = test->b;
	for(int i = 0; i < 1000000000; i++) {
		b += a;
	}
	a--;
	a++;
	int *a = 100;
	cout << a << enl;
}
